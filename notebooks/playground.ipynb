   "source": [
    "def PPformat(per):\n",
    "    try :\n",
    "        per == float(per)\n",
    "    except:\n",
    "        per = 0\n",
    "    new = str(format(per, '.1f')) + 'pp'\n",
    "    if per >= 0:\n",
    "        new = '+' + new\n",
    "        return new\n",
    "    else:\n",
    "        return new"
   ]
